#include<stdio.h>
int main()
{
	int a[20];
	int d=0;
	int n;
    int *p;
	printf("enter size of array\n");
	scanf("%d",&n);
	for (int i=0;i<n;i++)
	{
	
	if(i==0)
	{
	    printf("enter elements of array\n");
	    printf("size is %d\n",n);
	};
	scanf("%d",&a[i]);
	
	    
    };
    while(d<n)
    {
        printf("%d ",a[d]);
        d++;
    }
    
    printf("\n%d",a[0]);
    
     *p=a[0];
    printf("\n%d",p);
    *p=a[1];
    printf("\n%d",p);
	return 0;
}
